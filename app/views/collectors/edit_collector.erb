<h2> Edit Your Profile </h2>

<form method="post" action="/collectors/<%=@collector.id%>">
  <input type="hidden" id="hidden" name="_method" value="patch">
  <input type="text" name="email" value="<%=@collector.email%>">
  <input type="text" name="username" value="<%=@collector.username%>">
  <input type="password" name="password" placeholder="Enter or Update Password">
  <input type="submit" value="Edit">
</form>
