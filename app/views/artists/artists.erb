<h2> All Artists </h2>

  <%@artists.each do |artist|%>
    <%if artist.collectors.uniq.include?(@collector)%>
    <ul><h4><a href='/artists/<%=artist.id%>'><%=artist.name%></a></h4></ul>
  <%end%>
  <%end%>

  <br><br>

<form action= '/genres' method='get'>
    <input type='submit' value='Show All Genres' />
  </form>
<form action= '/artworks' method='get'> <input type='submit' value='Show All Artworks' /> </form>
